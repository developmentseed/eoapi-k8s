{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "required": [
    "service",
    "gitSha"
  ],
  "properties": {
    "service": {
      "type": "object",
      "required": [
        "port"
      ],
      "properties": {
        "port": {
          "type": "integer",
          "pattern": "^[0-9]{4}$"
        }
      }
    },
    "ingress": {
      "type": "object",
      "required": [
        "className"
      ],
      "properties": {
        "className": {
          "type": "string",
          "pattern": "^(nginx|testing123)$"
        }
      }
    },
    "gitSha": {
      "type": "string",
      "pattern": "^.+$"
    }
  }
}
